{
    "name": "baki/image",
    "description": "Store image",
    "type": "package",
    "homepage": "https://github.com/BojanDjurdjevic995/image",
    "license": "MIT",
    "authors": [
        {
            "name": "Bojan Djurdjevic",
            "email": "djurdjevicbojan12@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "illuminate/http" : "*",
        "illuminate/config": "*",
        "illuminate/filesystem": "*",
        "intervention/image" : "*"
    },
    "autoload": {
        "classmap": [
            "/"
        ],
        "psr-4": {
            "\\": "/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Baki\\Image\\ImageServiceProvider"
            ],
            "aliases": {
                "StoreImage": "Baki\\Image\\Facades\\ImageFacades"
            }
        }
    }
}
